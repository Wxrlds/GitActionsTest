<!--Remove Wooden Gear-->
    <recipe name="Gear, Wood" required="true" disabled="true">
    <crafting>
      <grid>
        <item /><item name="stickWood"/><item />
        <item name="stickWood"/><item /><item name="stickWood"/>
        <item /><item name="stickWood"/><item />
      </grid>
      <output name="GEAR_WOOD"/>
    </crafting>
  </recipe>

